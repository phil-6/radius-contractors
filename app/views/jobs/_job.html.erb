<div id="<%= dom_id job %>">
  <p>
    <strong>User:</strong>
    <%= job.user_id %>
  </p>

  <p>
    <strong>Contractor:</strong>
    <%= job.contractor_id %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= job.description %>
  </p>

  <p>
    <strong>State:</strong>
    <%= job.state %>
  </p>

  <p>
    <strong>Start date:</strong>
    <%= job.start_date %>
  </p>

  <p>
    <strong>End date:</strong>
    <%= job.end_date %>
  </p>

  <p>
    <strong>Review:</strong>
    <%= job.review %>
  </p>

</div>
